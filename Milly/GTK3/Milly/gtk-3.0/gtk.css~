/* GTK NAMED COLORS
   ----------------
   use responsibly! */
/*
widget text/foreground color */
@define-color theme_fg_color #717171;
/*
text color for entries, views and content in general */
@define-color theme_text_color #717171;
/*
widget base background color */
@define-color theme_bg_color #0f1013;
/*
text widgets and the like base background color */
@define-color theme_base_color #0f1013;
/*
base background color of selections */
@define-color theme_selected_bg_color #9d2a5e;
/*
text/foreground color of selections */
@define-color theme_selected_fg_color #D2D2D2;
/*
base background color of insensitive widgets */
@define-color insensitive_bg_color #0f1013;
/*
text foreground color of insensitive widgets */
@define-color insensitive_fg_color rgba(210, 210, 210, 0.5);
/*
insensitive text widgets and the like base background color */
@define-color insensitive_base_color #0f1013;
/*
widget text/foreground color on backdrop windows */
@define-color theme_unfocused_fg_color #717171;
/*
text color for entries, views and content in general on backdrop windows */
@define-color theme_unfocused_text_color #717171;
/*
widget base background color on backdrop windows */
@define-color theme_unfocused_bg_color #0f1013;
/*
text widgets and the like base background color on backdrop windows */
@define-color theme_unfocused_base_color #0f1013;
/*
base background color of selections on backdrop windows */
@define-color theme_unfocused_selected_bg_color #9d2a5e;
/*
text/foreground color of selections on backdrop windows */
@define-color theme_unfocused_selected_fg_color #D2D2D2;
/*
insensitive color on backdrop windows */
@define-color unfocused_insensitive_color rgba(210, 210, 210, 0.5);
/*
widgets main borders color */
@define-color borders rgba(0, 0, 0, 0.26);
/*
widgets main borders color on backdrop windows */
@define-color unfocused_borders rgba(0, 0, 0, 0.26);
/*
these are pretty self explicative */
@define-color warning_color #dcab90;
@define-color error_color #dc8bb0;
@define-color success_color #acdb90;
/*
these colors are exported for the window manager and shouldn't be used in applications,
read if you used those and something break with a version upgrade you're on your own... */
@define-color wm_title #717171;
@define-color wm_unfocused_title rgba(210, 210, 210, 0.7);
@define-color wm_highlight rgba(255, 255, 255, 0.1);
@define-color wm_bg #0f1013;
/*
FIXME this is really an API */
@define-color content_view_bg #0f1013;
@define-color placeholder_text_color #9e9e9e;
